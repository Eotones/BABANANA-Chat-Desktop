{
  "name": "babanana-chat-desktop",
  "version": "0.4.0",
  "description": "BABANANA Chat Desktop 為 BABANANA Chat 的桌面版應用程式。",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build_win64": "electron-packager . BABANANA-Chat-Desktop --out=./build/ --overwrite=true --platform=win32 --arch=x64 --version=0.36.4 --icon=assets/img/app.ico --asar=true --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune=true ",
    "build_win32": "electron-packager . BABANANA-Chat-Desktop --out=./build/ --overwrite=true --platform=win32 --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune=true "
  },
  "author": "EOTONES",
  "license": "MIT License",
  "devDependencies": {
    "electron": "^17.2.0",
    "electron-packager": "^15.4.0"
  },
  "dependencies": {
    "bootstrap-icons": "^1.8.1"
  }
}
